{
  "compilerOptions": {
    "jsx": "react-jsx",
    "noEmit": true,
    "target": "ES6",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "allowImportingTsExtensions": true,
    "lib": ["DOM", "ES2016"]
  },
  "include": ["src"],
  "exclude": ["node_modules", "dist"]
}
